#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_node-zip@1.1.1@node-zip/bin/nodezip" "$@"
  ret=$?
else 
  node  "$basedir/../../../_node-zip@1.1.1@node-zip/bin/nodezip" "$@"
  ret=$?
fi
exit $ret
